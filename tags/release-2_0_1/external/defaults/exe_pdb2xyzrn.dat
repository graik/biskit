[BINARY]

comment= modified awk script (that comes with msms) pdb_to_xyzrn to extract names and radii. gpdb_to_xyzrn uses gawk rather than nawk.

## Pdb2xyzrn will assign our modified biskit/external/msms/gpdb_to_xyzrn
## (unless bin is not None, so your own configuration would override that)
bin=

## working directory
cwd=

## wrap process in shell
shell=0

## use non-default shell
shellexe=

## communicate with process via STDIN and STDOUT
pipes=1

## Use new environment containing only variables given below
replaceEnv=0

##
## run time environment settings
## Empty entries mark variables that have to be specified from outside
##
[ENVIRONMENT]
