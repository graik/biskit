evaluate ($inputpsf   = "input.psf")
evaluate ($inputpdb   = "input.pdb")
evaluate ($param19    = "param19.pro" )
evaluate ($nbonds_repel = "nbonds_repel.inp" )
evaluate ($neutral19  = "neutral19.inp" )
evaluate ($pdb_out    = "final.pdb" )

structure @@$inputpsf end
coor @@$inputpdb

parameter
  	reset
        @@$param19
	@@$nbonds_repel
end
parameter nbonds wmin 0.1 end end


@$neutral19	!scale sicechain charges


flags include bonds angles dihed improper vdw elec cdih end

dynamics internal
  trajectory= rb_dyna.dat nsavc= 5 ascii = true
  nprint= 10

  endtime= 10
  nstep= 10000
  
  etolerance= 5

  tbath= 300
  
  group= (resid 1:69)
  group= (resid 84:145)

  AUTO TORSion

  itype= pc6

end

write coor output= $pdb_out end